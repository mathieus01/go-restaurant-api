{
  "name": "go-restaurant-api",
  "version": "1.3.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "typescript:watch": "tsc -w",
    "local": "cross-env NODE_ENV=development tsnd --transpile-only --ignore-watch node_modules --respawn src/main/server.ts",
    "test": "cross-env NODE_ENV=test jest --silent --passWithNoTests --noStackTrace --runInBand --no-cache --detectOpenHandles --forceExit",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests = true",
    "test:ci": "npm test -- --coverage",
    "db:migrate:test": "cross-env NODE_ENV=test npx sequelize db:migrate",
    "db:migrate:undo": "cross-env NODE_ENV=test npx sequelize db:migrate:undo:all"
  },
  "keywords": [],
  "author": "Matheus Nunes Barros",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.13",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/knex": "^0.15.0",
    "@types/node": "^14.6.3",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "eslint": "^7.8.1",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "git-commit-msg-linter": "^2.6.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "mockdate": "^3.0.2",
    "sequelize-cli": "^6.2.0",
    "sqlite3": "^5.0.0",
    "sucrase": "^3.16.0",
    "supertest": "^6.0.1",
    "ts-jest": "^26.3.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.2",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.1.0"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.12",
    "module-alias": "^2.2.2",
    "pg": "^8.5.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "ts-node": "^9.0.0",
    "validator": "^13.1.17"
  },
  "engines": {
    "node": "12.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
